{
    "name": "zendframework/zend-escaper",
    "description": " ",
    "license": "BSD-3-Clause",
    "keywords": [
        "zf2",
        "escaper"
    ],
    "homepage": "https://github.com/zendframework/zend-escaper",
    "autoload": {
        "psr-4": {
            "Zend\\Escaper\\": "src/"
        }
    },
    "require": {
        "php": ">=5.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "2.5-dev",
            "dev-develop": "2.6-dev"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZendTest\\Escaper\\": "test/"
        }
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "1.7.*",
        "phpunit/PHPUnit": "~4.0"
    },
    "support": {
        "docs": "https://zendframework.github.io/zend-escaper/"
    }
}
